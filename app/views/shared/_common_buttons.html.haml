.col-xs-12.nothing
  - search = Group.new
  - object = ["all", "discussions", "groups", "people"]
  = bootstrap_form_for(search, :remote => :true, layout: :inline ) do |f|
    .panel.panel-default
      -#.panel-heading
        .panel-title
          - if defined?(title)
            = title
            .button.close{:type => "button", "data-dismiss" => "alert", "aria-label" => "Close" }
          - else
            New Group
      .panel-body
        .btn-toolbar{ :role => "toolbar" }
