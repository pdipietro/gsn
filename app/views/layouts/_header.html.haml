.navbar.navbar-default.navbar-fixed-top{ :role => "navigation" }
  .container
    .snavbar-header
      %button.navbar-toggle.collapsed{ :type => "button", "data-toggle" => "collapse",  "data-target" => "#bs-example-navbar-collapse-1" }
        %span.sr-only  Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      %a.navbar-brand.logo{ :href => "#{root_path}" }
        - sn = "#{get_social_name}_logo.png"
        %img.banner.logo{ :src => "#{image_url(sn)}", :height => "46px" }
      %a.navbar-brand{ :href => "#{root_path}" }
        = "#{get_social_name.humanize}"
      - unless logged_in?
        %ul.nav.navbar-nav.navbar-left
          %li
            = link_to root_path do
              %span.glyphicons.icon-home
              Home
          %li
            = link_to login_path do
              %span.glyphicons.icon-log-in
              Log in
      - else
        %ul.nav.navbar-nav.navbar-right
          %li
            = link_to users_path do
              %span.glyphicons.glyphicons-user
              People
          %li.dropdown
            %a.dropdown-toggle{ :href => "#", "aria-expanded" => "false", "data-toggle" => "dropdown", :type => "button"} 
              %span.glyphicons.glyphicons-plus.pull-top.light
              Create new
              %span.caret
            %ul.dropdown-menu{ :role => "menu"}
              %li
                = link_to root_path do
                  %span.glyphicons.icon-group
                  Group
              %li
                = link_to root_path do
                  %span.glyphicons.icon-chat-bubble-two
                  Chat
              %li.divider
              %li
                = link_to root_path do
                  %span.glyphicons.icon-picture
                  Image
          %li.dropdown
            %a.dropdown-toggle{ :href => "#", "aria-expanded" => "false", "data-toggle" => "dropdown", :type => "button"}
              = gravatar_for current_user, size: 18
              = current_user.nickname
              %span.caret
            %ul.dropdown-menu{ :role => "menu" }
              %li
                = link_to root_path do
                  %span.glyphicons.icon-star-empty
                  My preferites
              %li
                = link_to root_path do
                  %span.glyphicons.icon-group
                  My groups
              %li
                = link_to root_path do
                  %span.glyphicons.icon-smiley-frown
                  My workgroups
              %li
                = link_to root_path do
                  %span.glyphicons.icon-network
                  My networks
              %li
                = link_to root_path do
                  %span.glyphicons.icon-chat-bubble-two
                  My chats
              %li
                = link_to root_path do
                  %span.glyphicons.icon-smiley-frown
                  My posts
              %li
                = link_to root_path do
                  %span.glyphicons.icon-like-2
                  My likes
              %li
                = link_to root_path do
                  %span.glyphicons.icon-picture
                  My images
              %li.divider
              %li
                = link_to edit_user_path(current_user) do
                  %span.glyphicons.icon-settings-streamline
                  Change profile
              %li
                = link_to current_user do
                  %span.glyphicons.glyphicons-user
                  Change password
              %li.divider
              %li
                = link_to current_user do
                  %span.glyphicons.icon-progress-3
                  Everythings
            %li
              = link_to logout_path, method: "delete" do
                %span.glyphicons.icon-log-out
                = 'Log out'



